class Solution {
    public int findUnsortedSubarray(int[] nums) {
        if (nums.length == 1) return 0;
        int min = 0;
        int max = 0;
        for (int i = 1; i < nums.length; ++i) {
            if (nums[i] < nums[min]) {
                min = i;
            }
            if (nums[i] > nums[max]) {
                max = i;
            } 
        }
        if (max)
        System.out.println(min + " " + max);
        return 0;
    }
}
